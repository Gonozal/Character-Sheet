         <RulesElement charelem="252cc628" internal-id="ID_FMP_CLASS_FEATURE_1544" legality="rules-legal" name="Centered Breath" type="Class Feature">
            <Category> ID_FMP_CLASS_FEATURE_1543,1 </Category>
    <specific name="Short Description"> Centered Flurry of Blows and Mental Equilibrium </specific>
    <specific name="Level"> 1 </specific>
    <specific name="Powers"> ID_FMP_POWER_7448 </specific>
    <specific name="type"> At-Will </specific>
    <specific name="Class"> ID_FMP_CLASS_362 </specific>
